{
    "manifest_version": 2,
    "name": "My Test Extension",
    "author": "Lukaswbrr",
    "version": "0.0.1",
    "content_scripts": [
        {
        "matches": ["*://*/*"]
        }
    ],
    "background": {
        "page": "background.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "currentvideotime2obsidian@lukaswbrr.com"
        }
    },
    "permissions": [
        "activeTab",
        "scripting",
        "tabs"
    ],
    "commands": {
        "test-print": {
            "suggested_key": {
                "default": "Ctrl+1",
                "mac": "Command+1"
            },
            "description": "Test feature"
        }
    }
}